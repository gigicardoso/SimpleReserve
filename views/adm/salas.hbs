<script>
document.addEventListener('DOMContentLoaded', function() {
	document.querySelectorAll('.btn-delete').forEach(function(btn) {
		btn.addEventListener('click', function(e) {
			if (!window.confirm('Tem certeza que quer excluir?')) {
				e.preventDefault();
			}
		});
	});
});
</script>
